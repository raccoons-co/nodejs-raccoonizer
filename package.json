{
  "name": "nodejs-raccoonizer",
  "version": "0.0.1",
  "description": "Runs Node.js application at Heroku",
  "main": "EntryPoint.ts",
  "scripts": {
    "build": "nest build",
    "lint": "npm run lint.typescript && npm run lint.packagejson",
    "lint.typescript": "eslint './**/*.ts'",
    "lint.packagejson": "npmPkgJsonLint .",
    "this.raccoonizer.microservice": "node dist/EntryPoint",
    "start": "npm test && npm run this.raccoonizer.microservice",
    "test": "npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raccoons-co/nodejs-raccoonizer.git"
  },
  "author": "Raccoons <info@raccoons.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raccoons-co/nodejs-raccoonizer/issues"
  },
  "homepage": "https://github.com/raccoons-co/nodejs-raccoonizer#readme",
  "dependencies": {
    "@hapi/hapi": "^20.2.2",
    "@nestjs/common": "^9.1.4",
    "@nestjs/core": "^9.1.4",
    "@nestjs/platform-fastify": "^9.1.4",
    "express": "^4.18.1",
    "koa": "^2.13.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.4",
    "@types/express": "^4.17.14",
    "@types/hapi__hapi": "^20.0.12",
    "@types/koa": "^2.13.5",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.25.0",
    "npm-package-json-lint": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "16.x"
  }
