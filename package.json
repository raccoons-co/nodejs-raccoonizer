{
  "name": "@raccoons-co/nodejs-raccoonizer",
  "version": "0.0.17",
  "description": "Node.js microservices library",
  "files": [
    "lib/"
  ],
  "main": "lib/main/index",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc",
    "build.docs": "typedoc ./src/main/index.ts",
    "lint.packagejson": "npmPkgJsonLint .",
    "lint.typescript": "eslint './src/**/*.ts'",
    "prebuild": "rimraf ./lib",
    "prebuild.docs": "rimraf ./docs",
    "prepare": "npm run build",
    "preprepare": "npm-run-all lint.*",
    "prestart": "npm ci",
    "start": "node lib/test/EntryPoint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raccoons-co/nodejs-raccoonizer.git"
  },
  "keywords": [
    "microservice",
    "nodejs",
    "framework",
    "koa",
    "hapi",
    "express"
  ],
  "author": "Raccoons <info@raccoons.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raccoons-co/nodejs-raccoonizer/issues"
  },
  "homepage": "https://github.com/raccoons-co/nodejs-raccoonizer#readme",
  "dependencies": {
    "@hapi/hapi": "^20.2.2",
    "express": "^4.18.1",
    "koa": "^2.13.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/hapi__hapi": "^20.0.12",
    "@types/koa": "^2.13.5",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.27.0",
    "npm-package-json-lint": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=16.x"
  }
}
